<?xml version="1.0" encoding="utf-8"?>
<!-- Para usar o modelo criado basta colocar no xml>TExt>android:background  -->
<!-- substirui o selector por shape por causa de suas bibliotecas de edição -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
    <!-- estilo da borda no caso retangular -->
    <!-- Tipo de borda -->
    <solid android:color="#FFFFFF"/>
    <!-- altura e cor da borda -->
    <stroke android:width="5dp"
            android:color="#00008B" />
    <!-- Formato e amplitude das bordas, deixar mai arredondadas -->
    <corners
             android:radius="15dp"/>
</shape>